{
    "name": "astar_demo",
    "version": "1.0.0",
    "description": "demo of the A* pathfinding algorithm",
    "private": true,
    "mode": "development",
    "scripts": {
        "prestart": "npm run build",
        "start": "concurrently \"webpack serve\" \"node scripts/open.js\"",
        "build": "npm run format && npm run build:clean && npm run build:js",
        "build:noformat": "npm run build:clean && npm run build:js",
        "build:clean": "node ./scripts/clean.js",
        "build:js": "webpack",
        "watch": "webpack --watch",
        "format": "npm run prettier:fix && npm run lint:fix",
        "lint:check": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "prettier:fix": "prettier --write src/**/*.ts",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist"
    },
    "keywords": [],
    "author": "",
    "homepage": "http://iamrichr.github.io/aStar_demo",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.16.12",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@types/p5": "^1.4.1",
        "@types/react": "^17.0.42",
        "@types/react-dom": "^17.0.14",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "@webpack-cli/generators": "^1.1.0",
        "babel-loader": "^8.2.3",
        "concurrently": "^7.1.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.10.0",
        "gh-pages": "^4.0.0",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.0",
        "path-browserify": "^1.0.1",
        "prettier": "^2.5.1",
        "sass": "^1.50.0",
        "sass-loader": "^12.6.0",
        "shelljs": "^0.8.5",
        "source-map-loader": "^3.0.1",
        "typescript": "^4.6.2",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1"
    },
    "dependencies": {
        "bulma": "^0.9.3",
        "p5": "^1.4.1",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    }
}
